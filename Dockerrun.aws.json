{
  "AWSEBDockerrunVersion": 2,
  "containerDefinitions": [
    {
      "name": "client",
      "image": "zoltot1/multi-client",
      "hostname": "client",
      "essential": false
    },
    {
      "name": "nginx",
      "image": "zoltot1/multi-nginx",
      "hostname": "nginx",
      "essential": true,
      "portMappings": [{
        "hostPort": 80,
        "containerPort": 80
      }],
      "link": ["client", "api"]
    },
    {
      "name": "server",
      "image": "zoltot1/multi-server",
      "hostname": "api",
      "essential": false
    },
    {
      "name": "worker",
      "image": "zoltot1/multi-worker",
      "hostname": "worker",
      "essential": false
    }
  ]
}